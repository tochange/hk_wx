.cropper_main_container {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 100;
    width: 750rpx;
    height: 100vh;
    background: #555;
    overflow: hidden;
    background: #1a2d27;
}

.cropper_main_container.hidden {
    display: none;
}

.cropper_container {
    position: absolute;
    left: 0;
    top: 0;
    display: flex;
    align-items: center;
    justify-content: center;
}

.cropper_toolbar {
    position: absolute;
    z-index: 100;
    bottom: 0;
    height: 50px;
    background: black;
    width: 100%;
    line-height: 50px;
    font-size: 15px;
    text-align: center;
    color: white;
}

.cropper_toolbar .button_item {
    float: left;
    width: 20%;
    height: 50px;
}

.original_button .check_container {
    position: relative;
    float: left;
    margin-top: 14px;
    width: 18px;
    height: 18px;
    border: 1px solid white;
}

.original_button.checked {
    background: #26ab28;
}

.crop_image_button {
    color: #26ab28;
}

.crop_image_button.disable {
    color: rgba(0,0,0,0);
}

.cropper_movable_area_container {
    position: relative;
}

.cropper_canvas_container_item {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.cropper_canvas_container {
    position: relative;
}

.cropper_movable_area_container .move_item {
    z-index: 110;
    background: white;
    border-radius: 50%;
}

.cropper_canvas_container .canvas {
    position: absolute;
    left: 0;
    top: 0;
}

.original_canvas {
    position: absolute;
    left: 750rpx;
    top: 100vh;
}

.scale-image {
    position: absolute;
    left: 0;
    top: 0;
}

.move_canvas {
}

page {
    background: #1a2d27;
}

.nexttap {
    position: absolute;
    width: 552rpx;
    height: 104rpx;
    font-size: 32rpx;
    color: #ffffff;
    bottom: 64rpx;
    border: 4px solid #8da8a1;
    text-align: center;
    line-height: 104rpx;
    left: 100rpx;
}